{
  "name": "users-api",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "backup": "ts-node scripts/backup.ts ./backup.jsonl",
    "restore": "ts-node scripts/restore.ts ./backup.jsonl"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.0",
    "@prisma/client": "^5.18.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "prisma": "^5.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}
